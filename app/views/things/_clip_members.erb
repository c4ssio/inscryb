<!--clip_members layer-->
<div class="clip_members" id="clip_member_wrapper">
  <table class="clip_members">
    <%@clip_members.each do |cm|%>
      <tr class="clip_members">
        <td class="clip_members">
          <div class="clip_members">
            <%=link_to_remote('Paste',
              :url=>{:action=>'retrieve',:clip_id=>cm.id, :op=>'paste'},
              :update=>'member_and_tag_wrapper')%>
            <%=link_to(cm.thing.name,thing_url(:id=>cm.thing_id))%>
            <%=link_to_remote('Cancel',
              :url=>{:action=>'clip_tag',:clip_id=>cm.id, :op=>'cancel'},
              :update=>'clip_member_wrapper')%>
            <%=cm.operation.name%>
          </div>
        </td>
      </tr>
    <%end%>
  </table>
</div>

